public class Task2 {
    public static void main(String[] args) {

        int[] intArray = new int[]{13, 43, 2134, 1, 341, 41543}; // size = 6;
        int index = 8;
        int d = 0;
        if (d != 0) {
            if (index > 0 && index < intArray.length) {
                double catchedRes1 = intArray[index] / d;
                System.out.println("catchedRes1 = " + catchedRes1);
            }
        }
    }
}
